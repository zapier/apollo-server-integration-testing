    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
